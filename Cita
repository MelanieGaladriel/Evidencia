import java.util.Date;
import java.time.LocalDateTime;



public class Cita {
    int id;
    Date fechaCita;
    String hora;
    int idDoctor;
    int idPaciente;
    String motivoCita;

    public Cita (int idC, Date fecha, String horacita, int doctorId, int pacienteId, String motivo){
      id = idC;
      fechaCita = fecha;
      hora = horacita;
      idDoctor = doctorId;
      idPaciente = pacienteId;
      motivoCita = motivo;
    }
}
